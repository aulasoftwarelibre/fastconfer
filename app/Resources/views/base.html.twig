    {% block stylesheets %}
        {% stylesheets
            "@bootstrap_css"
            "@select2_bootstrap_css_css"
            "@select2_dist_css"
            "@components_font_awesome_css"
            "@ionicons_css"
            "@admin_lte_css"
            "bundles/app/components/admin-lte/dist/css/skins/skin-blue.css"

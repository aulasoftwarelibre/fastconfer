{% extends 'base.html.twig' %}


{% block body %}

    <ul>
        {% if user is null %}

            <p>  Para poder Apuntarse a un congreso debe estar logueado</p>

             {% else %}
                <li>El usario: {{ user.username }}</li>
                <li>Con Correo: {{ user.email }}</li>
                <li>Se ha dado de alta en la conferencia: {{ conference.name }}</li>
                 <h1>Upload File</h1>

                 <form action="#" method="post" {{ form_enctype(form) }}>
                     {{ form_widget(form) }}

                     <input type="submit" value="Upload Document"/>
                 </form>

        {% endif %}
    </ul>





{% endblock %}
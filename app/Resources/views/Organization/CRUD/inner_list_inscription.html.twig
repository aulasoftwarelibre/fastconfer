{% extends 'SonataAdminBundle:CRUD:base_list_flat_inner_row.html.twig' %}

{% block row %}

    <div class="media-body" xmlns="http://www.w3.org/1999/html">

        <div class="media">
            <span class="glyphicon glyphicon-user"></span>
            {{ object|render_list_element(admin.list['user']) }}

            <span class="pull-right"><i class="fa fa-calendar"></i>
                {{ object|render_list_element(admin.list['createdAt']) }} </span>

        </div>
    </div>

{% endblock %}